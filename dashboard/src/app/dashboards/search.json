{
  "title": "API Response Monitoring: Search",
  "style": "light",
  "refresh": "1m",
  "dashname": "search",
  "history_paths":{
    "search_page":"/dashboard/file/search.json",
	"main_page":"/dashboard/file/main.json",
	"service_page":"/dashboard/file/service.json/sub",
	"api_page":"/dashboard/file/api.json/sub"
  },
  "index": {
    "interval": "none",
    "pattern": "[logstash-]YYYY.MM.DD",
    "default": "srm",
    "warm_fields": false
  },
  "services": {
	"query": {
		"list": {
		  "0": {
		    "query": "message:**",
		    "alias": "",
		    "color": "#7EB26D",
		    "id": 0,
		    "pin": false,
		    "type": "lucene",
		    "enable": true
		  }
		},
		"ids": [
		  0
		]
	},
    "filter": {
        "list": {
  		  "0": {
  			  "active": true,
  			  "type": "range",
  			  "field": "timestamp",
  		      "from": 1417171400,
  		      "to": 1417172400
  		  }
      },
      "ids": ["0"],
      "time": {
          "from": "now-1h",
          "to": "now"
      }
    }
  },
  "header": {
	  "panels": [
		  {
			  "span": 12,
			  "type": "dashheader",
			  "source": {}
		  }
	  ]
  },
  "rows": [
	{
		"height": "200px",
		"panels": [
			{
			  "title": "검색",
		      "span": 12,
		      "type": "search",
			  "href": "#/dashboard/file/service.json",
			  "param": "service",
				  "source": {
					  "column": "service",
					  "series": "dash.service.request_cnt",
					  "rawQuery":true,
					  "cache": true,
					  
			          "query": "select distinct(service) as service from dash.service.request_cnt",
				      "query1": "select count(distinct(hostname)) as total from dash.service.request_cnt where service='%SERVICE%'",
				      "query2": "select level, hostname, title, message from status.log where service='%SERVICE%' and time > now() - 20s",
					  "interval": 0
				  }
			}
		]
	}
  ],
  "nav": [
    {
      "type": "timepicker",
      "collapse": false,
      "notice": false,
      "enable": true,
      "status": "Stable",
      "time_options": [
        "5m",
        "15m",
        "1h",
		"2h",
		"3h",
        "6h",
        "12h",
        "24h"
      ],
      "refresh_intervals": [
        "10s",
        "30s",
        "1m"
      ],
      "now": true
    }
  ]
}
